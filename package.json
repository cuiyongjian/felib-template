{
  "name": "felib-template",
  "version": "1.0.0",
  "description": "A scaffold for the front end library",
  "author": "felib",
  "main": "index.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist && shx echo \"> clean finished!\"",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "devbuild": "cross-env NODE_ENV=development node build/build.js",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "node build/dev-server.js",
    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cross-env": "^5.1.5",
    "mocha": "^5.1.1",
    "ora": "^1.4.0",
    "semver": "^5.5.0",
    "shelljs": "^0.7.8",
    "shx": "^0.2.2",
    "webpack": "^3.12.0",
    "webpack-merge": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuiyongjian/felib-template.git"
  },
  "keywords": [
    "felib",
    "template"
  ],
  "bugs": {
